    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="vs"
              android:id="@+id/textView1"
              android:typeface="normal" android:textStyle="bold"
              android:layout_below="@+id/relativeLayout1" android:layout_centerHorizontal="true"
              android:layout_marginTop="10dp"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gen_text"
            android:background="@drawable/randbutton"
            android:id="@+id/button" android:layout_centerHorizontal="true" android:layout_below="@+id/relativeLayout"
